{
  "name": "portfolio-app",
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --colors --progress --display-error-details --display-cached --content-base src/",
    "test": "karma start --reporters dots",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "pree2e": "webdriver-manager update --standalone",
    "e2e": "protractor",
    "prebuild": "npm run test && npm run e2e",
    "build": "rimraf dist && webpack --config config/webpack.prod.config.js --progress --profile --bail",
    "postinstall": "typings install",
    "format": "clang-format --glob=\"src/**/*.ts\" -i --style=file"
  },
  "author": "Connor Wyatt <connorwyatt1@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@types/lodash": "^4.14.37",
    "angular2-hmr": "0.7.0",
    "core-js": "^2.4.1",
    "element-resize-detector": "^1.1.9",
    "lodash": "^4.16.4",
    "reset-css": "^2.2.0",
    "rxjs": "^5.0.0-rc.1",
    "ts-helpers": "1.1.1",
    "ui-router-ng2": "^1.0.0-beta.3",
    "ui-router-visualizer": "^2.0.7",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "clang-format": "^1.0.45",
    "copy-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.17.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.7.0",
    "null-loader": "^0.1.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.3",
    "typescript": "latest",
    "typings": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  }
}
